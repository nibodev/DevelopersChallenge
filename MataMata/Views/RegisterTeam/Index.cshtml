
@{
    ViewBag.Title = "Cadastrar Participantes";
}
<br />
<div id="geral" class="row container">
    <div id="consulta">
        <div class="row">
            <input id="btnAdd" type="button" class="btn btn-primary" value="Adicionar Novo" />
        </div>
        <div class="row">
            <table id="list"></table>
            <div id="pager"></div>
        </div>
    </div>
    <div id="cadastro" style="display:none">
        <div class="form-group">
            <label for="Name">Código</label>
            <input type="text" class="form-control" id="IdTeam" readonly="readonly">
        </div>
        <div class="form-group">
            <label for="Name">Nome</label>
            <input type="text" class="form-control" id="Name" style="text-transform:uppercase">
        </div>


        <input id="btnSalvar" type="button" class="btn btn-primary" value="Salvar" />
        <input id="btnExcluir" type="button" class="btn btn-info" value="Excluir" />
        <input id="btnCancelar" type="button" class="btn btn-danger" value="Voltar" />
    </div>
</div>

<script src="~/Scripts/AppScripts/registerTeam.js?revision_2"></script>